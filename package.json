{
  "name": "dealership-platform",
  "version": "2.0.0",
  "description": "AI-powered dealership inventory platform with quality scoring and RAG",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "migrate: "node scripts/migrate.js",
    "db:migrate": "node scripts/migrate-postgres.js",
    "db:seed": "node scripts/seed.js",
    "queue:worker": "node src/queue/workers/index.ts",
    "generate:embeddings": "node scripts/generate-embeddings.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "@pinecone-database/pinecone": "^1.1.0",
    "openai": "^4.20.0",
    "@anthropic-ai/sdk": "^0.6.0",
    "bull": "^4.11.0",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "prom-client": "^15.0.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0",
    "joi": "^17.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^3.0.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cls-hooked": "^4.4.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.17",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.4",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.10",
    "ts-jest": "^29.1.1",
    "eslint": "^8.54.0",
    "@typescript-eslint/parser": "^6.13.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "dealership",
    "inventory",
    "scraping",
    "ai",
    "rag",
    "vector-search",
    "quality-scoring",
    "postgresql",
    "openai",
    "pinecone"
  ],
  "author": "Alex Lajoie",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Crazyal55/ClawBotDealer1.git"
  }
}
